  "defaults": {
    "control": {
      "size": {
        "small": {
          "height": {
            "value": "5*{GU}",
            "type": "sizing"
          },
          "gap": {
            "value": "1*{GU}",
            "type": "spacing"
          },
          "padding": {
            "value": "2*{GU}",
            "type": "spacing"
          }
        },
        "step": {
          "height": {
            "value": "2*{GU}",
            "type": "other"
          },
          "padding": {
            "value": "{control.size.step.height}/2",
            "type": "other"
          },
          "gap": {
            "value": "{control.size.step.height}/2",
            "type": "other"
          }
        },
        "medium": {
          "height": {
            "value": "{control.size.small.height}+{control.size.step.height}",
            "type": "sizing"
          },
          "gap": {
            "value": "{control.size.small.gap}+{control.size.step.gap}",
            "type": "spacing"
          },
          "padding": {
            "value": "{control.size.small.padding}+{control.size.step.padding}",
            "type": "spacing"
          }
        },
        "large": {
          "height": {
            "value": "{control.size.medium.height}+{control.size.step.height}",
            "type": "sizing"
          },
          "gap": {
            "value": "{control.size.medium.gap}+{control.size.step.gap}",
            "type": "spacing"
          },
          "padding": {
            "value": "{control.size.medium.padding}+{control.size.step.padding}",
            "type": "spacing"
          }
        },
        "stepWrapper": {
          "padding": {
            "value": "{control.size.step.height}/2",
            "type": "spacing"
          }
        }
      },
      "input": {
        "shape": {
          "borderRadius": {
            "value": "6",
            "type": "borderRadius"
          }
        },
        "border": {
          "width": {
            "value": "1",
            "type": "borderWidth"
          },
          "focus": {
            "step": {
              "value": "1",
              "type": "other"
            }
          },
          "widthFocus": {
            "value": "{control.input.border.width} + {control.input.border.focus.step}",
            "type": "borderWidth"
          }
        }
      }
    }
  },
